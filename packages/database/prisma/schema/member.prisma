enum SNSType {
  discord
  github
  instagram
  solvedac
  email
}

model SNS {
  id        String    @id @default(uuid())
  type      String    
}

model Member {
  id            String      @id @default(uuid())
  generation    Int
  name          String
  department    String
  speciality     String
  introduction  String
  profile_image String
  discord       String?
  github        String?
  instagram     String?
  solvedac      String?
  email         String?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}